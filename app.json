{
  "name": "Discord-MusicBot",
  "description": "Very simple discord music bot with very easy-to-use commands and interface.",
  "repository": "https://github.com/joeyk710/Discord-MusicBot",
  "logo": "",
  "keywords": ["node", "discord", "youtube","music","bot"],
  "image": "heroku/nodejs",
  "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    },
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "TOKEN": {
      "description": "The token copied in the first section.",
